{
	"name": "Govno 3",
	"description": "Govno 3 addon aka penochka for 2-ch.ru site",
	"version": "3.0.8.63",
   "update_url": "http://github.com/anonymous32767/Penochka/raw/govno/govno_unstable.xml",
	"permissions": [
		"tabs",
		"http://2-ch.ru/*",
		"http://*.2-ch.ru/*",
		"http://02ch.su/*",
		"http://*.02ch.su/*",
		"http://iichan.ru/*",
		"http://*.iichan.ru/*"
	],
	"content_scripts": [
		{
			"matches": ["http://2-ch.ru/*", "http://*.2-ch.ru/*", "http://*.02ch.su/*", "http://02ch.su/*", "http://*.iichan.ru/*", "http://iichan.ru/*"],
			"run_at": "document_start",
			"js": ["govno.js"]
		}
	]
}
